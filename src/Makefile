# CREATES THE COMPILER FILE FOR ALL OF THE PROGRAMS
compiler: tokenizer.o main.o history.o

	cc -o compiler tokenizer.o main.o history.o

# DELETES FILES GENERATED BY COMPILATION

clean:

	rm -f *.o tokenizer


# INCLUDE FILE DEPENDENCIES

tokenizer.o: tokenizer.h tokenizer.c



uimain.o: main.c tokenizer.h history.h



history.o: history.h history.c tokenizer.h
