
# include file dependencies

main.o: main.c tokenizer.h history.h

tokenizer.o: tokenizer.c tokenizer.h

history.o: history.c history.h



# creates the compiler file for all of the programs

compiler: main.o tokenizer.o history.o
	cc -o compiler main.o tokenizer.o history.o

	cc -o



# deletes files generated by compilation

clean:
	rm -f *.o compiler

	rm -f *.o test
